pipeline {
    agent{
       label 'docker-slave'
    }
  
  environment {

    DOCKERHUB_CREDENTIALS = credentials('docker-credentials')
     registry = "yoursureshdockerhub/lmsbackend"
        registryCredential = 'docker-credentials'
        dockerImage = ''
    }
    
     stages {
        
         stage('creating  new docker network') {
            steps {
               
                sh 'docker network create lmsnetwork'
            }
          
          }
        }
    }
        
